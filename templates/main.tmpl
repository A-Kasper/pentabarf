<patTemplate:tmpl name="main">
<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <title>{PAGE_TITLE} {CONTENT_TITLE} - Pentabarf (r{REVISION})</title>
    <link rel="stylesheet" type="text/css" href="{BASE_URL}css/main.css"/>
    <link rel="stylesheet" type="text/css" href="{BASE_URL}js/jscalendar/calendar-blue.css"/>
    <script src="{BASE_URL}js/enumerator.js" type="text/javascript"></script>
    <script src="{BASE_URL}js/cookie.js" type="text/javascript"></script>
    <script src="{BASE_URL}js/switch_tab.js" type="text/javascript"></script>
    <script type="text/javascript" src="{BASE_URL}js/sorttable.js"></script>
<script type="text/javascript">
var data_base_url = '{BASE_URL}pentabarf/';
var image_base_url = '{BASE_URL}images/';
</script>
  </head>
  <body id="body">
    <patTemplate:tmpl name="sidebar" src="sidebar.tmpl" parse="on" />
    <div id="main">
      <div id="context">
        <h1>{CONTENT_TITLE}</h1>
        <patTemplate:tmpl name="person_navigation" src="person_navigation.tmpl" parse="on" visibility="hidden" />
        <patTemplate:tmpl name="event_navigation" src="event_navigation.tmpl" parse="on" visibility="hidden" />
      </div>
      <div id="content">
        <div id="tabs">
          <patTemplate:tmpl name="tabs" src="tabs.tmpl" parse="on" />
        </div>
        <div id="data">
            <patTemplate:link src="content" />
        </div>
      </div>
    </div>
    <patTemplate:tmpl name="activity" src="activity.tmpl" parse="on" />
  </body>
<script type="text/javascript">

var image_base_url = '{BASE_URL}images/';

<patTemplate:tmpl name="js_vars" parse="on"><patTemplate:var name="FUNCTION" modifier="Javascript" />
</patTemplate:tmpl>
</script>
<script type="text/javascript">

var init_done = false;

<patTemplate:tmpl name="init" parse="on"><patTemplate:var name="FUNCTION" modifier="Javascript" />
</patTemplate:tmpl>

init_done = true;
enumerator();

<patTemplate:tmpl name="register_handler" parse="off" visibility="hidden">
register_handler();
</patTemplate:tmpl>
</script>
  </html>
</patTemplate:tmpl>
