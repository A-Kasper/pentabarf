xml.div( :id => 'object-image' ) do
  xml.img( :src => image_path( 'icon-list-64x64.png' ), :class => 'object-image' )
end

xml.div( :id => 'data' ) do
  xml.fieldset do
    xml.ul({:class=>:recent_changes}) do

      @changes.each do | changeset |
        LogEntry.new( changeset, controller ).append( xml )
      end

    end
  end
end

